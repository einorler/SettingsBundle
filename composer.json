{
    "name": "ongr/settings-bundle",
    "description": "Settings user interface and API for ONGR platform projects",
    "type": "symfony-bundle",
    "homepage": "http://ongr.io",
    "license": "MIT",
    "authors": [
        {
            "name": "ONGR team",
            "homepage": "http://www.ongr.io"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "~2.7|~3.0",
        "doctrine/cache": "~1.4",
        "friendsofsymfony/jsrouting-bundle": "~1.5",
        "ongr/elasticsearch-bundle": "~1.0",
        "ongr/filter-manager-bundle": "~1.0",
        "ongr/cookies-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "squizlabs/php_codesniffer": "~2.0",
        "satooshi/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": { "ONGR\\SettingsBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
